{
  "name": "zw",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "docusaurus": "docusaurus",
    "start": "docusaurus start",
    "build": "docusaurus build",
    "build:en": "docusaurus build --locale en",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "write-docs-version": "docusaurus docs:version",
    "write-heading-ids": "docusaurus write-heading-ids",
    "write-translations": "docusaurus write-translations",
    "write-community-version": "docusaurus docs:version:community",
    "write-ecosystem-version": "docusaurus docs:version:ecosystem",
    "crowdin:sync": "docusaurus write-translations && crowdin upload sources && crowdin download",
    "build:cf": "npm i -g pnpm && pnpm -s install && pnpm -s run build",
    "clean": "pnpm -s exec rm -rf node_modules build",
    "preinstall": "npx only-allow pnpm",
    "prepare": "husky install",
    "check": "pnpm trunk check --all",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@docusaurus/core": "2.0.0-beta.20",
    "@docusaurus/plugin-ideal-image": "2.0.0-beta.20",
    "@docusaurus/plugin-pwa": "2.0.0-beta.20",
    "@docusaurus/preset-classic": "2.0.0-beta.20",
    "@docusaurus/theme-search-algolia": "2.0.0-beta.20",
    "@loadable/component": "5.15.2",
    "@mdx-js/react": "1.6.22",
    "clsx": "1.1.1",
    "prism-react-renderer": "1.3.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "rehype-katex": "4.0.0",
    "remark-math": "3.0.1"
  },
  "devDependencies": {
    "@algolia/client-search": "4.13.0",
    "@babel/core": "^7.17.10",
    "@docusaurus/module-type-aliases": "2.0.0-beta.20",
    "@loadable/babel-plugin": "^5.13.2",
    "@tsconfig/docusaurus": "1.0.5",
    "@types/node": "17.0.31",
    "@types/react": "18.0.9",
    "asciinema-player": "3.0.0",
    "husky": "7.0.4",
    "prop-types": "15.8.1",
    "typescript": "4.6.4"
  },
  "peerDependencies": {
    "@types/react": "^18.0.9",
    "prop-types": "^15.8.1"
  },
  "packageManager": "pnpm@7.0.1",
  "pnpm": {
    "peerDependencyRules": {
      "allowedVersions": {
        "@types/react": "18.0.9"
      }
    },
    "overrides": {
      "nth-check@<2.0.1": ">=2.0.1",
      "trim@<0.0.3": ">=0.0.3"
    }
  },
  "engines": {
    "node": ">=14",
    "pnpm": ">=6"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
