{
  "name": "zw",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "docusaurus": "docusaurus",
    "start": "docusaurus start",
    "build": "docusaurus build",
    "build:en": "docusaurus build --locale en",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "write-docs-version": "docusaurus docs:version",
    "write-heading-ids": "docusaurus write-heading-ids",
    "write-translations": "docusaurus write-translations",
    "write-community-version": "docusaurus docs:version:community",
    "write-ecosystem-version": "docusaurus docs:version:ecosystem",
    "crowdin:sync": "docusaurus write-translations && crowdin upload sources && crowdin download",
    "build:cf": "npm i -g pnpm && pnpm i --prefer-frozen-lockfile --strict-peer-dependencies --no-optional && pnpm run build",
    "clean": "pnpm -s exec rm -rf node_modules build pnpm-lock.yaml",
    "preinstall": "npx only-allow pnpm",
    "prepare": "husky install",
    "check": "pnpm trunk check --all",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@docusaurus/core": "0.0.0-5010",
    "@docusaurus/plugin-ideal-image": "0.0.0-5010",
    "@docusaurus/plugin-pwa": "0.0.0-5010",
    "@docusaurus/preset-classic": "0.0.0-5010",
    "@docusaurus/theme-search-algolia": "0.0.0-5010",
    "@loadable/component": "5.15.2",
    "@mdx-js/react": "1.6.22",
    "clsx": "1.1.1",
    "prism-react-renderer": "1.3.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "rehype-katex": "4.0.0",
    "remark-math": "3.0.1"
  },
  "optionalDependencies": {
    "@typescript-eslint/eslint-plugin": "5.23.0",
    "@typescript-eslint/parser": "5.23.0",
    "eslint": "8.15.0",
    "eslint-config-google": "0.14.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-react": "7.29.4",
    "stylelint": "^14.8.2",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard": "^25.0.0",
    "stylelint-csstree-validator": "^2.0.0",
    "postcss": "^8.4.13",
    "webpack": "5.72.1"
  },
  "devDependencies": {
    "@algolia/client-search": "4.13.0",
    "@babel/core": "7.17.10",
    "@docusaurus/module-type-aliases": "0.0.0-5010",
    "@loadable/babel-plugin": "5.13.2",
    "@tsconfig/docusaurus": "1.0.5",
    "@types/react": "18.0.9",
    "asciinema-player": "3.0.0",
    "husky": "8.0.1",
    "prop-types": "15.8.1",
    "typescript": "4.6.4"
  },
  "peerDependencies": {
    "@types/react": "^18.0.9",
    "prop-types": "^15.8.1"
  },
  "packageManager": "pnpm@7.1.0",
  "pnpm": {
    "peerDependencyRules": {
      "allowedVersions": {
        "@types/react": "18.0.9",
        "@mdx-js/react": "1.6",
        "rehype-katex": "5",
        "remark-math": "3",
        "react": "17"
      }
    },
    "overrides": {
      "nth-check@<2.0.1": ">=2.0.1",
      "trim@<0.0.3": ">=0.0.3"
    }
  },
  "engines": {
    "node": ">=14",
    "pnpm": ">=6"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
