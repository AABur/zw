{
  "name": "zw",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "docusaurus": "docusaurus",
    "start": "docusaurus start",
    "build": "docusaurus build",
    "build:en": "docusaurus build --locale en",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "write-docs-version": "docusaurus docs:version",
    "write-heading-ids": "docusaurus write-heading-ids",
    "write-translations": "docusaurus write-translations",
    "write-community-version": "docusaurus docs:version:community",
    "write-ecosystem-version": "docusaurus docs:version:ecosystem",
    "crowdin:sync": "docusaurus write-translations && crowdin upload sources && crowdin download",
    "build:cf": "npm i -g pnpm && pnpm -s i && pnpm -s build",
    "clean": "pnpm -s exec rm -rf node_modules build pnpm-lock.yaml",
    "preinstall": "npx only-allow pnpm",
    "prepare": "husky install",
    "check": "pnpx @trunkio/launcher check",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@docusaurus/core": "0.0.0-5077",
    "@docusaurus/plugin-ideal-image": "0.0.0-5077",
    "@docusaurus/plugin-pwa": "0.0.0-5077",
    "@docusaurus/preset-classic": "0.0.0-5077",
    "@docusaurus/theme-search-algolia": "0.0.0-5077",
    "@mdx-js/react": "1.6.22",
    "asciinema-player": "3.0.0",
    "clsx": "1.1.1",
    "husky": "8.0.1",
    "prism-react-renderer": "1.3.3",
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "devDependencies": {
    "@babel/core": "7.18.2",
    "@docusaurus/module-type-aliases": "0.0.0-5077",
    "@loadable/component": "5.15.2",
    "@trunkio/launcher": "1.1.1",
    "@tsconfig/docusaurus": "1.0.5",
    "@types/node": "17.0.36",
    "@types/react": "18.0.9",
    "@typescript-eslint/eslint-plugin": "5.26.0",
    "@typescript-eslint/parser": "5.26.0",
    "eslint": "8.16.0",
    "eslint-config-google": "0.14.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-react": "7.30.0",
    "stylelint": "14.8.5",
    "stylelint-config-prettier": "9.0.3",
    "stylelint-config-standard": "25.0.0",
    "stylelint-csstree-validator": "2.0.0",
    "typescript": "4.7.2",
    "webpack": "5.72.1"
  },
  "packageManager": "pnpm@7.1.7",
  "pnpm": {
    "auto-install-peers": "true",
    "peerDependencyRules": {
      "allowedVersions": {
        "@types/react": "18",
        "@mdx-js/react": "1.6",
        "rehype-katex": "6",
        "remark-math": "3",
        "react": "17"
      }
    },
    "overrides": {
      "nth-check@<2.0.1": ">=2.0.1",
      "trim@<0.0.3": ">=0.0.3"
    }
  },
  "engines": {
    "node": ">=16",
    "pnpm": ">=7"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
