{
  "name": "zw",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "docusaurus": "docusaurus",
    "start": "docusaurus start",
    "build": "docusaurus build",
    "build:en": "docusaurus build --locale en",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "write-docs-version": "docusaurus docs:version",
    "write-community-version": "docusaurus docs:version:community",
    "crowdin:sync": "docusaurus write-translations && crowdin upload && crowdin download",
    "build:cf": "test \"$CF_PAGES\" = 1 && npx pnpm install -r && npx pnpm build",
    "prebuild": "test \"$CI\" = true && pnpm install -r --no-prefer-frozen-lockfile || echo skipping pnpm install",
    "check": "trunk check",
    "clean": "pnpm -s exec rm -rf node_modules build",
    "preinstall": "npx only-allow pnpm",
    "prepare": "husky install",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@docusaurus/core": "0.0.0-4813",
    "@docusaurus/plugin-ideal-image": "0.0.0-4813",
    "@docusaurus/plugin-pwa": "0.0.0-4813",
    "@docusaurus/preset-classic": "0.0.0-4813",
    "@docusaurus/theme-search-algolia": "0.0.0-4813",
    "@mdx-js/react": "1.6.22",
    "clsx": "1.1.1",
    "prism-react-renderer": "1.3.1",
    "react": "18.0.0",
    "react-dom": "18.0.0"
  },
  "devDependencies": {
    "@algolia/client-search": "4.13.0",
    "@awesome-crowdin/crowdin-contributors": "0.0.4",
    "@babel/core": "7.17.8",
    "@crowdin/cli": "3.7.8",
    "@docusaurus/module-type-aliases": "0.0.0-4813",
    "@trunkio/launcher": "1.0.2",
    "@tsconfig/docusaurus": "1.0.5",
    "@types/react": "17.0.43",
    "husky": "7.0.4",
    "prop-types": "15.8.1",
    "typescript": "4.6.3",
    "webpack": "5.70.0"
  },
  "engines": {
    "node": ">=14",
    "pnpm": ">=6"
  },
  "pnpm": {
    "overrides": {
      "nth-check@<2.0.1": ">=2.0.1",
      "trim@<0.0.3": ">=0.0.3",
      "minimist@<=1.2.5": ">0.0.0",
      "axios@<=0.21.1": ">=0.21.2",
      "axios@<0.21.1": ">=0.21.1"
    }
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "peerDependencies": {
    "react": "*",
    "react-dom": "*"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "react-dom": {
      "optional": true
    }
  }
}
